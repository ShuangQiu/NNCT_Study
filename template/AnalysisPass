# use primetime
# 最長パスを求める
##################
# $nangate_db: Target Library
# $vg: vg file
# $name: name
# $slk: slk file

set link_path $nangate_db
read_verilog $vg
current_design $name
link
# 'create_clock -name clock -period 50
create_clock clock -period 50
set timing_save_pin_arrival_and_slack TRUE
update_timing
report_global_slack -max -nosplit > $slk
exit
